package tutorial;
import java.util.ArrayList;
import java.util.Map;
import java.util.HashMap;

public class Maps {
	public static void main(String[] args) {
		Map m = new HashMap();
		m.put("tim", 5);
		m.put("joe", "x");
		m.put(11,999);
		
		System.out.println(m);
		System.out.println(m.get("tim"));
	}

}
