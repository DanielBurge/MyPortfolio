package tutorial1;
// Classes blueprint for object
// private only accessible in this class


public class Dog {
	private String name;
	private int age;
	
	
	public Dog(String name, int age) {
		// this referencing to private attributes
		this.name = name;
		this.age = age;
	}
	public void speak() {
		System.out.println(" I am " + this.name + "and I am " + this.age + "  years old");
	}
	
	
	

}
