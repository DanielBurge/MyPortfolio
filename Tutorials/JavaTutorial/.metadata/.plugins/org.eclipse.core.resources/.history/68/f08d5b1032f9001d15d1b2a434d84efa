
public class IfStatements {
	public static void main(String[] args) {
	// if statement = performs a block of code if it's condition evaluates to true
		
		int age =12;
		if(age>=18) {
			System.out.println("You are an adult!");
			
			}
		else {
			System.out.print("You are a kid!");
		}
		
		
		
		
	}

}
